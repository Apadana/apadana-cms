<table class="apadana-table" id="apadana-account-members" cellpadding="0" cellspacing="0">
<thead>
  <tr>
    <th align="right">نام کاربری</th>
    <th width="20">بازدیدها</th>
    <th width="140">آخرین بازدید</th>
  </tr>
</thead>
<tbody>
[for list]  <tr class="{odd-even}">
	<td align="right">{name}</td>
	<td>{visits}</td>
	<td><span title="{past-time}">{lastvisit}</span></td>
  </tr>
[/for list]
</tbody>
</table>
<table id="apadana-account-profile" cellpadding="6" cellspacing="0">
  <tr>
    <td colspan="2" align="center"><img src="{avatar}"></td>
  </tr>
  <tr>
    <td width="95">نام کاربری</td>
    <td>{name}</td>
  </tr>
  <tr>
    <td>گروه کاربر</td>
    <td>{group}&nbsp;<img src="{group-icon}" alt="{group-name}" title="{group-name}" class="apadana-group-icon" /></td>
  </tr>
  <tr>
    <td>نام مستعار</td>
    <td>{alias}</td>
  </tr>
  <tr>
    <td>ملیت</td>
    <td><span dir="ltr">{nationality}</span></td>
  </tr>[location]
  <tr>
    <td>محل زندگی</td>
    <td>{location}</td>
  </tr>[/location]
   <tr>
    <td>جنسیت</td>
    <td>{gender}</td>
  </tr>
  <tr>
    <td>تاریخ عضویت</td>
    <td>{date}</td>
  </tr>
  <tr>
    <td>آخرین بازدید</td>
    <td>{lastvisit}</td>
  </tr>
  <tr>
    <td>بازدیدها</td>
    <td><b>{visits}</b> بار از زمان عضویت در سایت لاگین کرده اید.</td>
  </tr>[web]
  <tr>
    <td>وب سایت</td>
    <td><a href="{web}" target="_blank" rel="nofollow">وب سایت</a></td>
  </tr>[/web]
  <tr>
    <td>وضعیت</td>
    <td>[active]<font color="green">فعال</font>[/active][banned]<font color="red">اخراج شده</font>[/banned]</td>
  </tr>[signature]
  <tr>
    <td>امضا</td>
    <td>{signature}</td>
  </tr>[/signature][active][member]
  <tr>
    <td>تماس</td>
    <td>[module=contact-us]<a href="{a href='contact-us/{name}'}" rel="nofollow" title="ارسال ایمیل برای {name}">ارسال ایمیل</a>&nbsp;&nbsp;[/module][module=private-messages]&nbsp;&nbsp;<a href="{a href='private-messages/new/{name}'}" rel="nofollow" title="ارسال پیام خصوصی برای {name}">ارسال پیام خصوصی</a>[/module]</td>
  </tr>[/member][/active]
</table>